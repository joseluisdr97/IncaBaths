    <script type="text/javascript" src="~/Intensely/assets/js/jquery.mixitup.js"></script>
    <!-- Add fancyBox -->
    <script type="text/javascript" src="~/Intensely/assets/js/jquery.fancybox.pack.js"></script>
    <!-- counter -->
    <script src="~/Intensely/assets/js/waypoints.js"></script>
    <script src="~/Intensely/assets/js/jquery.counterup.js"></script>
    <!-- Wow animation -->
    <script type="text/javascript" src="~/Intensely/assets/js/wow.js"></script>
    <!-- progress bar   -->
    <script type="text/javascript" src="~/Intensely/assets/js/bootstrap-progressbar.js"></script>
    <!-- Custom js -->
    <script type="text/javascript" src="~/Intensely/assets/js/custom.js"></script>

