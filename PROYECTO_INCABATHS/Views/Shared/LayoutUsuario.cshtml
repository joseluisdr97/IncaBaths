    <link rel="stylesheet" type="text/css" href="~/Intensely/assets/css/slick.css" />
    <!-- Fancybox slider -->
    <link rel="stylesheet" href="~/Intensely/assets/css/jquery.fancybox.css" type="text/css" media="screen" />
    <!-- Animate css -->
    <link rel="stylesheet" type="text/css" href="~/Intensely/assets/css/animate.css" />
