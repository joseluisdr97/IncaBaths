
@{
    Layout = null;
}

@foreach (var item in Model)
{
    <tr>
        <td>@item.Fecha</td>
        <td>@item.Total</td>
        <td>
            <a class="mostrarDetalle btn btn-info" data-toggle="modal" data-target="#exampleModal" data-idreserva="@item.IdReserva"><span class="glyphicon glyphicon-eye-open"></span> Ver detalle</a>
        </td>
    </tr>
}

